      <xs:selector xpath=".//mstns:预定单" />
      <xs:field xpath="mstns:预定单号" />
    </xs:unique>
    <xs:unique name="入住单_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
