              <xs:element name="折扣" type="xs:decimal" minOccurs="0" />
              <xs:element name="折扣原因" type="xs:string" minOccurs="0" />
              <xs:element name="是否加床" type="xs:boolean" default="false" minOccurs="0" />
              <xs:element name="加床价格" type="xs:decimal" minOccurs="0" />
              <xs:element name="预收款" type="xs:decimal" minOccurs="0" />
              <xs:element name="预定人" type="xs:string" minOccurs="0" />
              <xs:element name="预定公司" type="xs:string" minOccurs="0" />
              <xs:element name="联系电话" type="xs:string" minOccurs="0" />
              <xs:element name="备注" type="xs:string" minOccurs="0" />
              <xs:element name="操作员" type="xs:string" minOccurs="0" />
              <xs:element name="业务员" type="xs:string" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="入住单">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="入住单号" type="xs:string" />
              <xs:element name="预定单号" type="xs:string" minOccurs="0" />
              <xs:element name="会员编号" type="xs:int" minOccurs="0" />
              <xs:element name="客房类型" type="xs:string" minOccurs="0" />
              <xs:element name="抵店时间" type="xs:dateTime" minOccurs="0" />
              <xs:element name="离店时间" type="xs:dateTime" minOccurs="0" />
